<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.reber.Numbers" android:versionCode="3" android:versionName="1.4">
	<application android:label="@string/app_name"
		android:debuggable="false" android:icon="@drawable/icon"
		android:theme="@android:style/Theme.NoTitleBar">
		<!-- <activity android:name=".Numbers"-->
		<activity android:name=".NumbersActivity" android:label="@string/app_name"
			android:configChanges="keyboardHidden|orientation"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

	</application>
	<uses-sdk android:minSdkVersion="3" />
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 